@page "/admin/faqManagement/createFaq"

@using Symphony.BlazorServerApp.Controls.FaqComponents
@using Symphony.BlazorServerApp.Controls.Utilities

<div class="container-fluid mb-5">
    <AuthorizeView Roles="admin">
        <Authorized Context="User">
            <h2 class="fw-bolder text-center fs">Create a Faq</h2>
          <CreateFaqComponent />
        </Authorized>
        <NotAuthorized>
            <UnauthorizedAccess />
        </NotAuthorized>
    </AuthorizeView>
</div>